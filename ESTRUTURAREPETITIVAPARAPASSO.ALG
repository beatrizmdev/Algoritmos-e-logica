Algoritmo "estruturaRepetitivaParaPasso"

Var
i, valInicial, valFinal: inteiro

Inicio
      escreva("Por qual número você quer iniciar a contagem regressiva? ")
      leia(valInicial)

      para i de valInicial ate valFinal passo -1 faca
           escreval(i)
           se i=0 entao
              escreva("GO!!")
           fimse
      fimpara

Fimalgoritmo