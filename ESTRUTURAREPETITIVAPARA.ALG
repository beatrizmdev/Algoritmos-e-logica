Algoritmo "estruturaRepetitivaPara"

Var
i, a, b, total, valor: inteiro

Inicio
      escreva("Quantos números você quer somar?")
      leia(b)
      a <- 1

      para i de a ate b faca
           escreva("digite o valor: ")
           leia(valor)
           total <- total + valor
      fimpara

escreva(total)
Fimalgoritmo